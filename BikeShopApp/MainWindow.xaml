<Window x:Class="BikeShopApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BikeShopApp"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="{StaticResource PKNU_Height}" Width="{StaticResource PKNU_Width}"
        Background="{DynamicResource background}"
        MinHeight="250" MinWidth="350">
    <Grid>
        <Frame x:Name="MainFrame" Source="/Menu.xaml" NavigationUIVisibility="Visible"/>
    </Grid>
</Window>
