<Page x:Class="BikeShopApp.Contact"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BikeShopApp"
      mc:Ignorable="d" 
      d:DesignHeight="400" d:DesignWidth="800"
      Title="Contact">

    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="10,30,0,0" TextWrapping="Wrap" Text="Sender : " VerticalAlignment="Top" Height="21" Width="62" TextAlignment="Right"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,66,0,0" TextWrapping="Wrap" Text="Message : " VerticalAlignment="Top" Height="24" Width="62" TextAlignment="Right"/>
        <TextBox Margin="77,30,10,0" TextWrapping="Wrap" Height="21" VerticalAlignment="Top"/>
        <TextBox Margin="77,66,10,10" TextWrapping="Wrap" AcceptsReturn="True" AcceptsTab="True"/>

    </Grid>
</Page>
