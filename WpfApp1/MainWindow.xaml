<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Sample Window" Height="530" Width="880" MinWidth="380" MinHeight="230" FontFamily="맑은 고딕">
    <StackPanel HorizontalAlignment="Center">
        <TextBlock TextWrapping="Wrap" Text="Test Block" Height="20" Margin="10, 5"/>
        <TextBox x:Name="txtNames" Height="30" TextWrapping="Wrap" Text="Input something" Margin="5, 5"/>
        <ProgressBar x:Name="pgbTest" Height="30" Margin="10" Minimum="0" Maximum="100"/>
        <Slider Height="40" Margin="10"/>
        <PasswordBox Password="test password"  Margin="10, 5"/>

    </StackPanel>
</Window>
